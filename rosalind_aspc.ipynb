    "import math\n",
    "\n",
    "sum = 0\n",
    "for k in range(m,n+1):\n",
    "  comb = math.factorial(n)//(math.factorial(k)*math.factorial(n-k))\n",
    "  sum += comb\n",
    "modulo_ = sum%1000000\n",
    "modulo_"
